{
    "version": "11.7.0",
    "homepage": "https://tools.pdf24.org/en/creator",
    "description": "Free and easy to use PDF software with several functions",
    "license": {
        "identifier": "Freeware",
        "url": "https://www.pdf24.org/en/terms-of-use"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.pdf24.org/pdf24-creator-11.7.0s.msi#/Setup.msi_",
            "hash": "54ddece0085bd57501fade33746341ca4fe8a459ddc633508c1dad7355ef75ae"
        }
    },
    "pre_install": [
        "if (!(is_admin)) { error \"$app requires admin rights to $cmd\"; break }",
        "Start-Process 'msiexec' -Wait -Verb 'RunAs' -ArgumentList ('/i', \"$dir\\Setup.msi_\", '/qn'); Stop-Process -Name 'pdf24' -Force -ErrorAction 'SilentlyContinue'"
    ],
    "pre_uninstall": [
        "if (!(is_admin)) { error \"$app requires admin rights to $cmd\"; break }",
        "Stop-Process -Name 'pdf24' -Force -ErrorAction 'SilentlyContinue'; Start-Process 'msiexec' -Wait -Verb 'RunAs' -ArgumentList ('/x', \"$dir\\Setup.msi_\", '/qn')",
        "Start-Sleep -Seconds 2"
    ],
    "checkver": "Version\\s([\\d.]+)",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.pdf24.org/pdf24-creator-$versions.msi#/Setup.msi_",
                "hash": {
                    "url": "https://creator.pdf24.org/listVersions.php",
                    "find": "<td>$sha256</td>"
                }
            }
        }
    }
}
