    "extract_dir": "DocFetcher-1.1.26-Windows-64bit-Portable",
    "installer": {
        "script": [
            "Copy-Item \"$dir\\misc\\DocFetcher.bat\" \"$dir\\DocFetcher.bat\""
        ]
    },
    "bin": [
        "DocFetcher.exe",
        "docfetcher-daemon-windows.exe",
        "docfetcher.bat"
    ],
    "shortcuts": [
        [
            "DocFetcher.bat",
            "DocFetcher",
            "",
            "DocFetcher.exe"
        ]
    ],
    "persist": "conf",
    "post_install": [
        "Remove-Item \"$dir\\docfetcher-daemon-windows.exe\""
    ],
    "checkver": {
        "url": "https://sourceforge.net/projects/docfetcher/files/docfetcher/",
        "regex": "/docfetcher/([\\d.]+)/DocFetcher-[\\d.]+-Windows-64bit-Setup.exe"
    },
    "autoupdate": {
        "url": "https://sourceforge.net/projects/docfetcher/files/docfetcher/$version/DocFetcher-$version-Windows-64bit-Portable.zip",
        "hash": {
            "mode": "sourceforge"
        },
        "extract_dir": "DocFetcher-$version"
    }
